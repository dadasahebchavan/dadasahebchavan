  </welcome-file-list>
  <servlet>
  <servlet-name>1</servlet-name>
  <servlet-class>com.rt.Addservlet</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>1</servlet-name>
  <url-pattern>/add</url-pattern>
  </servlet-mapping>
  <servlet>
  <servlet-name>2</servlet-name>
  <servlet-class>com.rt.Updateservlet</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>2</servlet-name>
  <url-pattern>/Update</url-pattern>
  </servlet-mapping>
   <servlet>
   
  <servlet-name>3</servlet-name>
  <servlet-class>com.rt.Deleteservlet</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>3</servlet-name>
  <url-pattern>/delete</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>4</servlet-name>
  <servlet-class>com.rt.Selectallservlet</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>4</servlet-name>
  <url-pattern>/all</url-pattern>
  </servlet-mapping>
  
</web-app>